{
    "name": "Title Picker",
    "description":" Get the Title of the Tab",
    "version":"1.0",
    "manifest_version":3,
    "permissions":[
        "tabs"
        ],
   
    
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16": "/image/logo_16.png",
        "32": "/image/logo_32.png",
        "48":"/image/logo_48.png"
    }
    
}